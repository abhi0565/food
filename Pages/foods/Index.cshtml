@page
@model food.Pages_foods.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.fooditem[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fooditem[0].PowerAcquisitionDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fooditem[0].test)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fooditem[0].type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fooditem[0].origin)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.fooditem) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PowerAcquisitionDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.test)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.origin)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.ID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
